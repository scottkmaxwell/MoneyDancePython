import typing
import java.lang

T = typing.TypeVar("T")


class DbxGlobalCallbackFactory:
    def __init__(self) -> None: ...
    
    def createNetworkErrorCallback(self, s: str) -> 'DbxNetworkErrorCallback': ...
    
    def createRouteErrorCallback(self, s: str, t: 'T') -> typing.Any: ...
    
    
class DbxNetworkErrorCallback:
    def __init__(self) -> None: ...
    
    def onNetworkError(self, c: 'com.dropbox.core.DbxException') -> None: ...
    
    
class DbxRouteErrorCallback(java.lang.Runnable):
    def __init__(self) -> None: ...
    
    def getRouteError(self) -> 'T': ...
    
    def setRouteError(self, t: 'T') -> None: ...
    
    
